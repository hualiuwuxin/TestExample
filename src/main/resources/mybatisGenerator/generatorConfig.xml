<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>

	<context id="DB2Tables" targetRuntime="MyBatis3">
		
		<!-- <commentGenerator>
		        <property name="suppressDate" value="true"/>
		        <property name="suppressAllComments" value="true" />
		</commentGenerator> -->
		
		<commentGenerator type="com.lomi.mybatisGenerator.MyCommentGenerator">
        </commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://10.118.3.1:3313/uk-user?characterEncoding=utf8"
			userId="root" password="myPassswd123">
		</jdbcConnection>

		<!--生成entity类存放位置 -->
		<javaModelGenerator
			targetPackage="com.xyebank.uk.user.dao.entity"
			targetProject="TestExample">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!--生成映射文件存放位置 -->
		<sqlMapGenerator targetPackage="mapper"
			targetProject="TestExample/src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!--生成Dao类存放位置 -->
		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.xyebank.uk.user.service.mapper"
			targetProject="TestExample">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>


			
			
		<!-- 表映射 -->
		<table tableName="user_org" domainObjectName="UserOrg"
			enableCountByExample="false" enableUpdateByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false"
			selectByExampleQueryId="false">
		</table>
	

	</context>
</generatorConfiguration>